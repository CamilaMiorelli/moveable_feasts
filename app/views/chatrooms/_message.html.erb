<div class="message-container" id="message-<%= message.id %>">
      <p class="message-content"><%= message.content %></p>
      <i class="author">
        <span><%= message.user.first_name %></span>
        <small class="user-time"><%= message.created_at.strftime("%a %b %e at %l:%M%p") %></small>
      </i>
    </div>
