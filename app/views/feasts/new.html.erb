<h2>Create your Feast!</h2>
<%= simple_form_for @feast do |f| %>
    <%= f.input :title %>
    <%= f.input :description %>
    <%= f.input :meal_type %>
    <%= f.input :guest_limit %>
    <%= f.input :price %>
    <%= f.input :address %>
    <%= f.input :photo, as: :file %>
    <%= f.input :start_at, as: :datetime %>
    <%= f.input :end_at, as: :datetime %>

    <%= f.input :photo, as: :file %>
    <%= f.button :submit %>

<% end %>
